'elm_lang@core@Main@count'/2 = fun (_n, _x) ->
	letrec
		'recursive'/1 = fun (_shadow) ->
			call 'Runtime':'apply' ('elm_lang@core@Main@count'/2, _shadow)
	in
		letrec
			'shadow'/0 = fun () ->
				apply 'recursive'/1 (0)
		in
			case _n of
				<0> when 'true' ->
					let <_1> =
						apply 'shadow'/0 ()
					in
						call 'Runtime':'apply' (_1, _x)
				<_2> when 'true' ->
					apply 'elm_lang@core@Main@count'/2 (_n, _x)
			end
